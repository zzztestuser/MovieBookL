<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SessionServlet</servlet-name>
  	<display-name>SessionServlet</display-name>
  	<description></description>
  	<servlet-class>com.moviebook.servlet.SessionServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserFriendsServlet</servlet-name>
  	<display-name>UserFriendsServlet</display-name>
  	<servlet-class>com.moviebook.servlet.UserFriendsServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RecommendedMoviesServlet</servlet-name>
  	<display-name>RecommendedMoviesServlet</display-name>
  	<description></description>
  	<servlet-class>com.moviebook.servlet.RecommendedMoviesServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MovieInterestedFriendsServlet</servlet-name>
  	<display-name>MovieInterestedFriendsServlet</display-name>
  	<description></description>
  	<servlet-class>com.moviebook.servlet.MovieInterestedFriendsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SessionServlet</servlet-name>
  	<url-pattern>/api/session</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserFriendsServlet</servlet-name>
  	<url-pattern>/api/users/friends</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RecommendedMoviesServlet</servlet-name>
  	<url-pattern>/api/movies/recommended</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MovieInterestedFriendsServlet</servlet-name>
  	<url-pattern>/api/movies/interested/friends</url-pattern>
  </servlet-mapping>
</web-app>
